{
  "name": "@track-player/web",
  "version": "1.0.0",
  "description": "Audio player for React web applications with equalizer, visualization, and Media Session support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "audio",
    "player",
    "track",
    "music",
    "equalizer",
    "react",
    "web",
    "media-session"
  ],
  "author": "ItsLhuis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ItsLhuis/track-player.git",
    "directory": "packages/web"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@track-player/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "react": "^19.1.0",
    "typescript": "^5.9.3"
  }
}
